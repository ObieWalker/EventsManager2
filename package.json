{
  "name": "event-manager",
  "version": "1.0.0",
  "scripts": {
    "start": "babel-watch server/app.js",
    "start:dev": "nodemon --exec npm run babel-cli ./bin/www",
    "build": "babel src -d dist",
    "mocha": "mocha --compilers js:babel-core/register",
    "test": "nyc --reporter=html --reporter=text mocha ./server/tests/ --compilers js:babel-register --exit",
    "lint": "./node_modules/.bin/eslint",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "engines": {
    "node": "6.11.0"
  },
  "dependencies": {
    "auth-js": "0.0.10",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "coveralls": "^3.0.0",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "^4.15.5",
    "express-validator": "^4.3.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.1.0",
    "morgan": "~1.9.0",
    "nyc": "^11.3.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.12",
    "sequelize-cli": "^3.1.0",
    "serve-favicon": "~2.4.5",
    "sqlite3": "^3.1.13",
    "validator": "^9.1.2"
